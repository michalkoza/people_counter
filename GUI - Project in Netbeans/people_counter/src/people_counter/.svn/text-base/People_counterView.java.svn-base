    @Action
    public void showAboutBox() {
        if (aboutBox == null) {
            JFrame mainFrame = People_counterApp.getApplication().getMainFrame();
            aboutBox = new People_counterAboutBox(mainFrame);
            aboutBox.setLocationRelativeTo(mainFrame);
        }
        People_counterApp.getApplication().show(aboutBox);
    }

